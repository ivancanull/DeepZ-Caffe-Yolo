# 11 (upsample 2x, 20x12 -> 40x24)
layer {
  name: "upsample"
  type: "Deconvolution"
  bottom: "conv10"
  top: "upsample"
  param {
    lr_mult: 0
    decay_mult: 0
  }
  convolution_param {
    num_output: 128
	group: 128
    kernel_size: 4
    stride: 2
    pad: 1
    weight_filler {
      type: "bilinear"
    }
	bias_term: false
  }
}


# 12 (concatenate, 40x24x128 + 40x24x256 -> 40x24x384)
layer {
  name: "concat"
  type: "Concat"
  bottom: "upsample"
  bottom: "conv5"
  top: "concat"
  concat_param {
    axis: 1
  }
}


# 13 (40x24 -> 40x24)
layer {
  name: "conv11"
  type: "Convolution"
