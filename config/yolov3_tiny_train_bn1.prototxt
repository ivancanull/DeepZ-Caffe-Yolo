# detection
layer {
  name: "yolo_detection_loss"
  type: "YoloDetectionLoss"
  bottom: "det_conv1"
  bottom: "det_conv2"
  bottom: "det_label_data"
  top: "con_loss"
  top: "pos_loss"
  top: "cls_loss"
  yolo_detection_loss_param {
	loss_param {
	  detection_param {
        anchors {
          width: 0.1947
          height: 0.1971
        }
        anchors {
          width: 0.3245
          height: 0.4063
        }
        anchors {
          width: 0.8269
          height: 0.7668
        }
        classes: 0
	  }
	  ignore_threshold: 0.7
	}
	loss_param {
      detection_param {
        anchors {
          width: 0.0241
          height: 0.0336
        }
        anchors {
          width: 0.0553
          height: 0.0649
        }
        anchors {
          width: 0.0890
          height: 0.1394
        }
        classes: 0
      }
      ignore_threshold: 0.7
    }
  }
}
